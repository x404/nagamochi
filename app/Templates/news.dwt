<!-- =modal callback -->
<div class="modal-callback sidemodal" id="modal-callback" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="inner" role="document">
		<p class="title">CALL BACK</p>

		<form action="#" method="post" class="callback__form">
			<fieldset>
				<div class="select-wrapper">
					<select name="reviewobject" class="reviewobject" id="selectreview" placeholder="Country">
						<option value="Autstralia">Autstralia</option>
						<option value="Albania">Albania</option>
						<option value="Angola">Angola</option>
						<option value="Armenia">Armenia</option>
						<option value="Russia">Russia</option>
						<option value="Spain">Spain</option>
						<option value="Japan">Japan</option>
						<option value="USA">USA</option>
						<option value="Ukraine">Ukraine</option>
						<option value="New zeland">New zeland</option>
					</select>
				</div>
				<div class="input-field">
					<label for="mcallback_name" class="name-field">Your Name</label>
					<label for="mcallback_name" class="helper-field helper-field-name">Your Name</label>
					<input type="email" id="mcallback_name" name="name" class="form-control required" value="">
				</div>
				<div class="input-field">
					<label for="mcallback_tel" class="name-field">Your Phone</label>
					<label for="mcallback_tel" class="helper-field helper-field-name">Your Phone</label>
					<input type="tel" id="mcallback_tel" name="tel" class="form-control tel required" value="">
				</div>
				<div class="input-field textarea-field">
					<label for="mcallback_msg" class="name-field">Your Enquiry</label>
					<label for="mcallback_msg" class="helper-field helper-field-msg">Your Enquiry</label>
					<textarea id="mcallback_msg" name="msg" class="form-control textarea required"></textarea>
				</div>

				<div class="captcha">
					<img src="../tmp/captcha.jpg" alt="" />
				</div>

				<button type="submit" class="submit btn btn-black"><span>Continue</span></button>
			</fieldset>
		</form>
		<button type="button" data-dismiss="modal" class="close-modal" aria-label="Close">
			<svg width="24" height="24">
				<line x1="0" y1="0" x2="24" y2="24"/>
				<line x1="24" y1="0" x2="0" y2="24"/>
			</svg>
		</button>
	</div>
</div>
